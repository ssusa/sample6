@model IEnumerable<sample6.Models.SampleTable>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IntColumn1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntColumn2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VarcharColumn1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VarcharColumn2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VarcharColumn3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedColumn1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyColumn2)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IntColumn1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntColumn2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VarcharColumn1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VarcharColumn2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VarcharColumn3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedColumn1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyColumn2)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
